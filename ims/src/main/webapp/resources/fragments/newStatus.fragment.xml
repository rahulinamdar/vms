<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="newStatusFrom" minWidth="1024" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" title="Region Detail"
			labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" class="editableForm">
			<f:content>
				<Label text="Status Id" required="true"/>
				<Input required="true" value="{ path:'newStatus>/statusId' }"/>
				<Label text="Status Description"/>
				<Input value="{newStatus>/status}"></Input>
				
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>