<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.vasudha.controller.Orders"
	xmlns:html="http://www.w3.org/1999/xhtml">
			<Page title="Title">
				<content>
					
					<IconTabBar
		id="idIconTabBar"
		select="handleIconTabBarSelect"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter
				showAll="true"
				count="{/ProductCollectionStats/Counts/Total}"
				text="All Products"
				key="All" />
			<IconTabSeparator />
			<IconTabFilter
				icon="sap-icon://begin"
				iconColor="Positive"
				count="{/ProductCollectionStats/Counts/Weight/Ok}"
				text="Ok"
				key="Ok" />
			<IconTabFilter
				icon="sap-icon://compare"
				iconColor="Critical"
				count="{/ProductCollectionStats/Counts/Weight/Heavy}"
				text="Heavy"
				key="Heavy" />
			<IconTabFilter
				icon="sap-icon://inventory"
				iconColor="Negative"
				count="{/ProductCollectionStats/Counts/Weight/Overweight}"
				text="Overweight"
				key="Overweight" />
				
		</items>
	</IconTabBar>
			<Table id="adminStockTable"
			
				items="{ 
					path: 'stock>/stock',
					sorter : {
            			path : 'regionId',
            			group : true
        			}
				}" 
				columns="{
					path: 'columns>/' 
				}">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>products}" level="H2"/>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column styleClass="{columns>styleClass}" demandPopin="{columns>demandPopin}" minScreenWidth="{columns>minScreenWidth}"
						popinDisplay="WithoutHeader">
						<Text text="{columns>header}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle" type="Navigation">
						<cells>
							
							<ObjectIdentifier
								title="{stock>materialDesc}"
								text="{stock>materialId}" />
						
							<Text text="{stock>regionId}" wrapping="false"/>
							<Text text="{stock>total}" wrapping="false"/>
							
							<ObjectNumber
						number="{stock>availableQuantity}"
						unit="{stock>uom}"
						state="{
							path: 'stock>availableQuantity',
							formatter: '.formatter.qtyState'
						}" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
				</content>
			</Page>
</mvc:View>