{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.vasudha",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.vasudha.view.home",
			"type": "XML"
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.34",
			"libs": {
				"sap.ui.core": {
					"minVersion":"1.34.0"
				},
				"sap.m": {
					"minVersion":"1.34.0"
				},
				"sap.ui.layout": {
					"minVersion":"1.34.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.vasudha.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.vasudha.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [
				{
					"pattern": "landing",
					"name": "landing",
					"target": "landing"
				},
				{
					"pattern": "",
					"name": "admin",
					"target": "admin"
				},
				{
					"pattern": "stock",
					"name": "stock",
					"target": "stock"
				},
				{
					"pattern": "products",
					"name": "products",
					"target": [
						"master"
					]
				},
				{
					"pattern": "products/{productId}",
					"name": "productDetail",
					"target":  [
						"master",
						"detail"
					]
				},
				{
					"pattern": "login",
					"name": "login",
					"target": "login"
				},
				{
					"pattern": "shopOrders",
					"name": "shopOrders",
					"target": "shopOrders"
				},
				{
					"pattern": "shopOrders/{orderId}",
					"name": "orderDetail",
					"target": "orderDetail"
				},
				{
					"pattern": "pos",
					"name": "pos",
					"target": "pos"
				},
					{
					"pattern": "region",
					"name": "region",
					"target": "region"
				},
					{
					"pattern": "dump",
					"name": "dump",
					"target": "dump"
				},
					{
					"pattern": "adminSplit",
					"name": "adminSplit",
					"target": [
						"materList"
					]
				},
					{
					"pattern": "adminSplit/uom",
					"name": "uomCreate",
					"target": [
					"materList",
						"uomCreate"
					]
				},
					{
					"pattern": "adminSplit/category",
					"name": "categoryCreate",
					"target": [
					"materList",
						"categoryCreate"
					]
				},
					{
					"pattern": "adminSplit/region",
					"name": "regionCreate",
					"target": [
					"materList",
						"regionCreate"
					]
				},
					{
					"pattern": "adminSplit/status",
					"name": "statusCreate",
					"target": [
					"materList",
						"statusCreate"
					]
				}
			],
			"targets": {
				"landing": {
					"viewName": "landing",
					"viewLevel": 1
				},
				"admin": {
					"viewName": "admin",
					"viewLevel": 2
				},
				"stock": {
					"viewName": "stock",
					"viewLevel": 3
				},
				"parentSplitApp": {
					"viewPath": "com.vasudha.view",
					"viewName": "products",
					"viewId": "prodSplitApp"
				},
				"master": {
					"parent": "parentSplitApp",
					"viewName": "productList",
					"viewId": "prodList",
					"controlId": "prodSplitApp",
					"controlAggregation": "masterPages"
				},
				"detail": {
					"parent": "parentSplitApp",
					"viewName": "productDetail",
					"viewId": "prodDetail",
					"controlId": "prodSplitApp",
					"controlAggregation": "detailPages"
				},
				"login": {
					"viewName": "login",
					"viewLevel": 4
				},
				"shopOrders": {
					"viewName": "Orders",
					"viewLevel": 5
				},
				"orderDetail": {
					"viewName": "orderDetails",
					"viewLevel": 6
				},
				"pos": {
					"viewName": "POS",
					"viewLevel": 7
				},
				"region":{
					"viewName":"region",
					"viewLevel":8
				},
				"dump":{
					"viewName":"dump",
					"viewLevel":9
				},
				"parentAdminSplitApp": {
					"viewPath": "com.vasudha.view",
					"viewName": "adminSplit",
					"viewId": "adminSplit"
				},
				"materList":{
					"parent":"parentAdminSplitApp",
					"controlId":"adminSplit",
					"controlAggregation":"masterPages",
					"viewName":"adminActions"
				},
				"uomCreate":{
					"parent": "parentAdminSplitApp",
					"controlId": "adminSplit",
					"controlAggregation": "detailPages",
					"viewName":"uomCreate"
				},
				"statusCreate":{
					"parent": "parentAdminSplitApp",
					"controlId": "adminSplit",
					"controlAggregation": "detailPages",
					"viewName":"statusCreate"
				},
				"categoryCreate":{
					"parent": "parentAdminSplitApp",
					"controlId": "adminSplit",
					"controlAggregation": "detailPages",
					"viewName":"categoryCreate"
				},
				"regionCreate":{
					"parent": "parentAdminSplitApp",
					"controlId": "adminSplit",
					"controlAggregation": "detailPages",
					"viewName":"region"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}