{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.vasudha",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		}
	},

	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"

		]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.vasudha.view.home",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.vasudha.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.vasudha.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [
				{
					"pattern": "",
					"name": "landing",
					"target": "landing"
				},{
					"pattern":"/admin",
					"name":"admin",
					"target":"admin"
				},{
					"pattern":"/stock",
					"name":"stock",
					"target":"stock"
				},{
					"pattern": "products",
					"name": "products",
					"target": [
						"master",
						"detail"
					]
				}
			],
			"targets": {
				"landing": {
					"viewName": "landing",
					"viewLevel": 1
				},
				"admin":{
					"viewName":"admin",
					"viewLevel":2
				},
				"stock":{
					"viewName":"stock",
					"viewLevel":3
				},
				"parentSplitApp": {
					"viewPath": "com.vasudha.view",
					"viewName": "products",
					"viewId": "prodSplitApp"
				},
				"master": {
					"parent": "parentSplitApp",
					"viewName": "productList",
					"viewId": "prodList",
					"controlId": "prodSplitApp",
					"controlAggregation": "masterPages"
				},
				"detail": {
					"parent": "parentSplitApp",
					"viewName": "productDetail",
					"viewId": "prodDetail",
					"controlId": "prodSplitApp",
					"controlAggregation": "detailPages"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}